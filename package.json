  "scripts": {
    "start": "cd server && node dist/app",
    "build": "npm run build-frontend && npm run build-backend",
    "install": "npm run install-frontend && npm run install-backend && rm -rf ./node_modules",
    "test-frontend": "cd client && ng test --watch false",
    "build-frontend": "cd client && ng build --output-path ../server/public",
    "build-backend": "cd server && tsc",
    "install-frontend": "cd client && npm install",
    "install-backend": "cd server && npm install"
